<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
	<display-name>Starflag_bank</display-name>
	<welcome-file-list>
		<welcome-file>Login.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>StarFlag</servlet-name>
		<servlet-class>com.starflag.Servlet.StarFlagServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StarFlag</servlet-name>
		<url-pattern>/StarFlag</url-pattern>
	</servlet-mapping>
    </web-app>
